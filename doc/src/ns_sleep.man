[moddesc {NaviServer Built-in Commands}]



[titledesc {ns_sleep - Sleep for a specified number of seconds}]

[description]

 Sleeps for the number of seconds specified in seconds.


[section {COMMANDS}]

[list_begin definitions]


 [call [cmd ns_sleep] [arg seconds]]


[list_end]

[section EXAMPLES]

[example_begin]
   % ns_sleep 2
[example_end]


[see_also nsd]


[manpage_end]

